	Keywords = {Flight, Animal, Biomechanics, Animals, Sciuridae},
	Language = {English},
	Local-Url = {file://localhost/Users/dennisevangelista/Documents/Papers/2006/Bishop/J.%20Exp.%20Biol.%202006%20Bishop.pdf},
	Month = {Feb},
