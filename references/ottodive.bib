	Doi = {10.1242/jeb.01706},
	Journal = {J. Exp. Biol.},
	Keywords = {Analysis of Variance, Animals, Perciformes, Biomechanics, Video Recording, Swimming, Tail, Observation},
	Language = {English},
	Local-Url = {file://localhost/Users/dennisevangelista/Documents/Papers/2005/Standen/J.%20Exp.%20Biol.%202005%20Standen.pdf},
